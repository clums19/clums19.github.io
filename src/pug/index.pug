doctype html
html(lang='en')

    head

        meta(charset='utf-8')
        meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
        meta(name='description' content='')
        meta(name='author' content='')

        title Stylish Portfolio - Start Bootstrap Template

        // Favicon
        link(rel='icon', type='image/x-icon', href='assets/favicon.ico')

        // Font Awesome icons (free version)
        script(src='https://use.fontawesome.com/releases/v5.15.3/js/all.js', crossorigin='anonymous')

        // Simple line icons
        link(href='https://cdnjs.cloudflare.com/ajax/libs/simple-line-icons/2.5.5/css/simple-line-icons.min.css', rel='stylesheet')

        // Google fonts
        link(href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,700,300italic,400italic,700italic' rel='stylesheet' type='text/css')

        // Core theme CSS (includes Bootstrap)
        link(href='css/styles.css', rel='stylesheet')

    body#page-top

        // Navigation
        a.menu-toggle.rounded(href='#')
            i.fas.fa-bars
        nav#sidebar-wrapper
            ul.sidebar-nav
                li.sidebar-brand
                    a(href='#page-top') Start Bootstrap
                li.sidebar-nav-item
                    a(href='#page-top') Home
                li.sidebar-nav-item
                    a(href='#about') About
                li.sidebar-nav-item
                    a(href='#services') Services
                li.sidebar-nav-item
                    a(href='#portfolio') Portfolio
                li.sidebar-nav-item
                    a(href='#contact') Contact

        // Header
        header.masthead.d-flex.align-items-center
            .container.px-4.px-lg-5.text-center
                h1.mb-1 Chelsea Lumsden
                h3.mb-5
                    em Full-Stack Software Engineer
                a.btn.btn-primary.btn-xl(href='#about') Find Out More

        // About
        section#about.content-section.bg-light
            .container.px-4.px-lg-5.text-center
                .row.gx-4.gx-lg-5.justify-content-center
                    .col-lg-10
                        h2 A Full-Stack Softwear Engineer who is ready and excited to contribute to your next project!
                        p.lead.mb-5
                            | This site will show you a preview of all of the projects I have worked on past and present!
                            | You can also find more about be through my social apps below.
                        a.btn.btn-dark.btn-xl(href='#services') What I Offer

        // Services
        section#services.content-section.bg-primary.text-white.text-center
            .container.px-4.px-lg-5
                .content-section-heading
                    h3.text-secondary.mb-0 Languages
                    h2.mb-5 What I Offer
                .row.gx-4.gx-lg-5
                    .col-lg-3.col-md-6.mb-5.mb-lg-0
                        span.service-icon.rounded-circle.mx-auto.mb-3
                            img.img-icon(src='assets/img/jshtmlcss.png', alt='...' width="220")
                        h4
                            strong HTML5, Javascript, CSS3
                        p.text-faded.mb-0 Looks great on any screen size!
                    .col-lg-3.col-md-6.mb-5.mb-lg-0
                        span.service-icon.rounded-circle.mx-auto.mb-3
                            img.img-icon(src='assets/img/ruby1.png', alt='...' width="150") 
                        h4
                            strong Ruby and Ruby on Rails
                        p.text-faded.mb-0 Freshly redesigned for Bootstrap 5.
                    .col-lg-3.col-md-6.mb-5.mb-md-0
                        span.service-icon.rounded-circle.mx-auto.mb-3
                            img.img-icon(src='assets/img/react.png', alt='...' width="120") 

                        h4
                            strong React
                        p.text-faded.mb-0
                            | Millions of users
                            i.fas.fa-heart
                            | Start Bootstrap!
                    .col-lg-3.col-md-6
                        span.service-icon.rounded-circle.mx-auto.mb-3
                            img.img-icon(src='assets/img/node1.png', alt='...' width="150") 
                            
                        h4
                            strong Nodejs, Express
                        p.text-faded.mb-0 I mustache you a question...

        // Callout
        section.callout
            .container.px-4.px-lg-5.text-center
                h2.mx-auto.mb-5
                    | Welcome to
                    em your
                    | next website!
                a.btn.btn-primary.btn-xl(href='https://startbootstrap.com/theme/stylish-portfolio/') Download Now!

        // Portfolio
        section#portfolio.content-section
            .container.px-4.px-lg-5
                .content-section-heading.text-center
                    h3.text-secondary.mb-0 Portfolio
                    h2.mb-5 Recent Projects
                .row.gx-0
                    .col-lg-6
                        a.portfolio-item(href='https://atlien.netlify.app/')
                            .caption
                                .caption-content
                                    .h2 ATLien
                                    p.mb-0 An easy access application that has multiple must visit places in and near the city of Atlanta.
                            img.img-fluid(src='assets/img/atlien.png', alt='...')
                    .col-lg-6
                        a.portfolio-item(href='https://designtime2.herokuapp.com/')
                            .caption
                                .caption-content
                                    .h2 Design Time 2.0
                                    p.mb-0 A social platform that displays upcoming artist and their carnival costume designs.
                            img.img-fluid(src='assets/img/designtime.png', alt='...' )
                    .col-lg-6
                        a.portfolio-item(href='https://virtual-melanin-museum.netlify.app/')
                            .caption
                                .caption-content
                                    .h2 Virtual Melanin Musuem
                                    p.mb-0 This is an App will help Users look through various Black Artist and their art works throughout time.
                            img.img-fluid(src='assets/img/vvm.png', alt='...' )
                    .col-lg-6
                        a.portfolio-item(href='https://code-green-app.herokuapp.com/')
                            .caption
                                .caption-content
                                    .h2 Code Green
                                    p.mb-0  A group project working on a fictitious messaging application for companies to use internally.
                            img.img-fluid(src='assets/img/codegreen.png', alt='...')

        // Call to Action
        section.content-section.bg-primary.text-white
            .container.px-4.px-lg-5.text-center
                h2.mb-4 The buttons below are impossible to resist...
                a.btn.btn-xl.btn-light.me-4(href='#!') Click Me!
                a.btn.btn-xl.btn-dark(href='#!') Look at Me!

        // Map
        #contact.map
            iframe(src='https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d216735.17563635437!2d-84.53897875919537!3d33.765981304149875!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x88f5045d6993098d%3A0x66fede2f990b630b!2sAtlanta%2C%20GA%2C%20USA!5e0!3m2!1sen!2ses!4v1629018790049!5m2!1sen!2ses')
            br
            small
                a(href='https://maps.google.com/maps?f=q&source=embed&hl=en&geocode=&q=Twitter,+Inc.,+Market+Street,+San+Francisco,+CA&aq=0&oq=twitter&sll=28.659344,-81.187888&sspn=0.128789,0.264187&ie=UTF8&hq=Twitter,+Inc.,+Market+Street,+San+Francisco,+CA&t=m&z=15&iwloc=A')

        // Footer
        footer.footer.text-center
            .container.px-4.px-lg-5
                ul.list-inline.mb-5
                    li.list-inline-item
                        a.social-link.rounded-circle.text-white.mr-3(href='#!')
                            i.icon-social-facebook
                    li.list-inline-item
                        a.social-link.rounded-circle.text-white.mr-3(href='https://www.linkedin.com/in/chelsea-lumsden-112623210/')
                            i.icon-social-linkedin
                    li.list-inline-item
                        a.social-link.rounded-circle.text-white(href='https://github.com/clums19')
                            i.icon-social-github
                p.text-muted.small.mb-0 Copyright &copy; Chelsea's Portfolio 2021

        // Scroll to Top Button
        a.scroll-to-top.rounded(href='#page-top')
            i.fas.fa-angle-up

        // Bootstrap core JS
        script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.bundle.min.js')

        // Core theme JS
        script(src='js/scripts.js')